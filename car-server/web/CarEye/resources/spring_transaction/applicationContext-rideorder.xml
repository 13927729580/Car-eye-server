<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="baseDao" class="com.careye.base.dao.impl.GenericDaoImpl">
		<property name="sqlMapClient" ref="sqlMapClient" />
	</bean>
	<bean id="rideOrderServiceImpl" class="com.careye.transaction.service.RideOrderService"
		abstract="true">
		<property name="baseDao">
			<ref bean="baseDao" />
		</property>
		<property name="logService"  ref="logService" />
	</bean>
	<bean id="rideOrderService" class="com.careye.transaction.service.impl.RideOrderServiceImpl"
		parent="rideOrderServiceImpl">
	</bean>

</beans>
