<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="userLoginJson" extends="qhtaxijson" namespace="/userLoginJson">
	
		<action name="userLogin" method="userLogin"
			class="com.careye.common.action.UserLoginAction">
			<result name="success" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
			<result name="error" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
		</action>
		
		<action name="logout" method="logout"
			class="com.careye.common.action.UserLoginAction">
			<result name="success" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
			<result name="error" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
		</action>
		<action name="getVeryCode" method="getVeryCode"
			class="com.careye.common.action.UserLoginAction">
			<result name="success" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
			<result name="error" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
		</action>
		<action name="editPassWord" method="editPassWord"
			class="com.careye.common.action.UserLoginAction">
			<result name="success" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
			<result name="error" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
		</action>
		
	</package>


</struts>
