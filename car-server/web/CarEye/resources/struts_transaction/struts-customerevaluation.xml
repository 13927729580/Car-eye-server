<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<!-- json包 -->
	<package name="customerevaluationjson" extends="qhtaxijson" namespace="/customerevaluationjson">
	<!-- 司机评价客户信息 -->
	<action name="*" method="{1}"
			class="com.careye.transaction.action.CustomerEvaluationAction">
			<result name="success" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
			<result name="error" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
	</action>
	
	<action name="exportExcel" method="exportExcel"
			class="com.careye.transaction.action.CustomerEvaluationAction">
	</action>
	</package>
	
</struts>
